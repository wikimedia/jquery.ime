{

	"globals": {
		"QUnit": false,
		"rangy": false,
		"_": false
	},

	// Enforcing
	"bitwise": true,
	"curly": true,
	"eqeqeq": true,
	"freeze": true,
	"latedef": "nofunc",
	"futurehostile": true,
	"immed": true,
	"newcap": true,
	"noarg": true,
	"nonew": true,
	"noempty": true,
	"quotmark": "single",
	"undef": true,
	"unused": true,
	"strict": true,

	// Relaxing
	"plusplus": false,
	"laxbreak": true,
	"-W100": true,

	// Environment
	"browser": true,
	"jquery": true
}
