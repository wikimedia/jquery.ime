{
	"root": true,
	"extends": [
		"wikimedia/client",
		"wikimedia/jquery",
		"wikimedia/jsduck"
	],
	"globals": {
		"jQuery": "readonly",
		"$": "off"
	},
	"rules": {
		"camelcase": "off",
		"comma-spacing": "off",
		"indent": "off",
		"max-len": "off",
		"one-var": [ "error", { "var": "always" } ]
	}
}
