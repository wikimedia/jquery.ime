{
	"extends": "wikimedia",
	"parserOptions": {
		"ecmaVersion": 6
	},
	"env": {
		"browser": true,
		"es6": true,
		"jquery": true,
		"node": true,
		"qunit": true
	},
	"globals": {
		"require": false
	},
	"rules": {
		"array-bracket-spacing": 0,
		"camelcase": 0,
		"comma-spacing": 0,
		"curly": 0,
		"dot-notation": 0,
		"eol-last": 0,
		"eqeqeq": 0,
		"indent": 0,
		"key-spacing": 0,
		"keyword-spacing": 0,
		"max-len": 0,
		"new-cap": 0,
		"new-parens": 0,
		"no-cond-assign": 0,
		"no-implicit-coercion": 0,
		"no-irregular-whitespace": 0,
		"no-multi-spaces": 0,
		"no-prototype-builtins": 0,
		"no-return-assign": 0,
		"no-sequences": 0,
		"no-tabs": 0,
		"no-trailing-spaces": 0,
		"no-undef": 0,
		"no-underscore-dangle": 0,
		"no-unused-expressions": 0,
		"no-unused-vars": 0,
		"no-use-before-define": 0,
		"no-useless-escape": 0,
		"object-curly-spacing": 0,
		"one-var": [ "error", { "var": "always" } ],
		"operator-linebreak": 0,
		"quote-props": 0,
		"quotes": 0,
		"semi": 0,
		"semi-spacing": 0,
		"space-before-blocks": 0,
		"space-before-function-paren": 0,
		"space-in-parens": 0,
		"space-infix-ops": 0,
		"space-unary-ops": 0,
		"spaced-comment": 0,
		"switch-colon-spacing": 0,
		"valid-jsdoc": 0,
		"vars-on-top": 0,
		"wrap-iife": 0,
		"yoda": 0
	}
}
