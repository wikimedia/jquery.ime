{
	"extends": "wikimedia",
	"parserOptions": {
		"ecmaVersion": 6
	},
	"env": {
		"browser": true,
		"es6": true,
		"jquery": true,
		"node": true,
		"qunit": true
	},
	"globals": {
		"require": false
	},
	"rules": {
		"camelcase": 0,
		"comma-spacing": 0,
		"indent": 0,
		"max-len": 0,
		"no-multi-spaces": 0,
		"no-use-before-define": 0,
		"no-useless-escape": 0,
		"one-var": [ "error", { "var": "always" } ],
		"space-infix-ops": 0,
		"space-unary-ops": 0,
		"spaced-comment": 0,
		"switch-colon-spacing": 0,
		"valid-jsdoc": 0,
		"vars-on-top": 0,
		"wrap-iife": 0,
		"yoda": 0
	}
}
