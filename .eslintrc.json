{
	"root": true,
	"extends": [
		"wikimedia/client",
		"wikimedia/jquery",
		"wikimedia/jsdoc"
	],
	"globals": {
		"jQuery": "readonly",
		"$": "off"
	},
	"rules": {
		"camelcase": [ "error", { "allow": [ "patterns_x", "patterns_shift" ] } ],
		"indent": "off",
		"max-len": "off",
		"no-var": "off"
	}
}
